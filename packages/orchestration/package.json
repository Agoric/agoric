  ],
  "author": "Agoric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Agoric/agoric-sdk/issues"
  },
  "homepage": "https://github.com/Agoric/agoric-sdk#readme",
  "devDependencies": {
    "@agoric/zoe": "^0.26.2",
    "ava": "^5.3.0"
  },
  "ava": {
    "require": [
      "@endo/init/debug.js"
    ]
  },
  "files": [
    "*.js",
    "src"
  ],
  "publishConfig": {
    "access": "public"
