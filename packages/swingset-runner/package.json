{
  "name": "@agoric/swingset-runner",
  "version": "0.22.2",
  "private": true,
  "description": "Application to launch SwingSet instances for development and testing",
  "type": "module",
  "main": "./src/main.js",
  "repository": "https://github.com/Agoric/agoric-sdk",
  "author": "Agoric",
  "license": "Apache-2.0",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "run-s --continue-on-error 'lint:*'",
    "lint:eslint": "eslint .",
    "ci:autobench": "./autobench.js"
  },
  "dependencies": {
    "@agoric/assert": "workspace:*",
    "@agoric/deploy-script-support": "workspace:*",
    "@agoric/ertp": "workspace:*",
    "@agoric/inter-protocol": "workspace:*",
    "@agoric/internal": "workspace:*",
    "@agoric/kmarshal": "workspace:*",
    "@agoric/stat-logger": "workspace:*",
    "@agoric/store": "workspace:*",
    "@agoric/swing-store": "workspace:*",
    "@agoric/swingset-vat": "workspace:*",
    "@agoric/telemetry": "workspace:*",
    "@agoric/vat-data": "workspace:*",
    "@agoric/zoe": "workspace:*",
    "@endo/bundle-source": "^3.0.1",
    "@endo/eventual-send": "^1.0.1",
    "@endo/init": "^1.0.1",
    "@endo/marshal": "^1.0.1",
    "@endo/nat": "^5.0.1",
    "expose-gc": "^1.0.0",
    "n-readlines": "^1.0.1",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "ava": "^5.3.0",
    "c8": "^7.13.0",
    "import-meta-resolve": "^2.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "2m"
  },
  "typeCoverage": {
    "atLeast": 53.14
  }
}
