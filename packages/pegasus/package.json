{
  "name": "@agoric/pegasus",
  "version": "0.7.12",
  "description": "Peg-as-us contract",
  "type": "module",
  "main": "./src/pegasus.js",
  "engines": {
    "node": ">=14.15.0"
  },
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "yarn run --top-level run-s --continue-on-error 'lint:*'",
    "lint:eslint": "yarn run --top-level eslint .",
    "lint:types": "yarn run --top-level tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agoric/agoric-sdk.git"
  },
  "author": "Agoric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Agoric/agoric-sdk/issues"
  },
  "homepage": "https://github.com/Agoric/agoric-sdk#readme",
  "dependencies": {
    "@agoric/assert": "workspace:*",
    "@agoric/ertp": "workspace:*",
    "@agoric/internal": "workspace:*",
    "@agoric/network": "workspace:*",
    "@agoric/notifier": "workspace:*",
    "@agoric/store": "workspace:*",
    "@agoric/swingset-vat": "workspace:*",
    "@agoric/vats": "workspace:*",
    "@agoric/zoe": "workspace:*",
    "@endo/bundle-source": "^3.0.1",
    "@endo/captp": "^4.0.1",
    "@endo/far": "^1.0.1",
    "@endo/init": "^1.0.1",
    "@endo/nat": "^5.0.1",
    "@endo/promise-kit": "^1.0.1"
  },
  "devDependencies": {
    "ava": "^5.3.0",
    "c8": "^7.13.0",
    "import-meta-resolve": "^2.2.1"
  },
  "files": [
    "scripts/",
    "src/",
    "exported.js",
    "NEWS.md"
  ],
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "timeout": "10m"
  },
  "publishConfig": {
    "access": "public"
  },
  "typeCoverage": {
    "atLeast": 90.04
  }
}
