{
  "name": "@agoric/inter-protocol",
  "version": "0.16.1",
  "description": "Core cryptoeconomy contracts",
  "type": "module",
  "main": "src/index.js",
  "engines": {
    "node": ">=14.15.0"
  },
  "scripts": {
    "build": "yarn build:bundles",
    "build:bundles": "node ./scripts/build-bundles.js",
    "prepack": "tsc --build tsconfig.build.json",
    "postpack": "git clean -f '*.d.ts*' src/types.js",
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "run-s --continue-on-error 'lint:*'",
    "lint:eslint": "eslint .",
    "lint:types": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agoric/agoric-sdk.git"
  },
  "author": "Agoric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Agoric/agoric-sdk/issues"
  },
  "homepage": "https://github.com/Agoric/agoric-sdk#readme",
  "dependencies": {
    "@agoric/assert": "workspace:*",
    "@agoric/ertp": "workspace:*",
    "@agoric/governance": "workspace:*",
    "@agoric/internal": "workspace:*",
    "@agoric/notifier": "workspace:*",
    "@agoric/store": "workspace:*",
    "@agoric/time": "workspace:*",
    "@agoric/vat-data": "workspace:*",
    "@agoric/vats": "workspace:*",
    "@agoric/zoe": "workspace:*",
    "@endo/captp": "^4.0.1",
    "@endo/eventual-send": "^1.0.1",
    "@endo/far": "^1.0.1",
    "@endo/marshal": "^1.0.1",
    "@endo/nat": "^5.0.1",
    "@endo/promise-kit": "^1.0.1",
    "jessie.js": "^0.3.2"
  },
  "devDependencies": {
    "@agoric/smart-wallet": "workspace:*",
    "@agoric/swingset-liveslots": "workspace:*",
    "@agoric/swingset-vat": "workspace:*",
    "@agoric/zone": "workspace:*",
    "@endo/bundle-source": "^3.0.1",
    "@endo/init": "^1.0.1",
    "@endo/promise-kit": "^1.0.1",
    "@fast-check/ava": "^1.1.5",
    "ava": "^5.3.0",
    "c8": "^7.13.0",
    "deep-object-diff": "^1.1.9",
    "import-meta-resolve": "^2.2.1"
  },
  "files": [
    "scripts",
    "src/",
    "exported.js",
    "NEWS.md"
  ],
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "require": [
      "@endo/init/debug.js"
    ],
    "workerThreads": false,
    "timeout": "10m"
  },
  "publishConfig": {
    "access": "public"
  },
  "typeCoverage": {
    "atLeast": 93.45
  }
}
