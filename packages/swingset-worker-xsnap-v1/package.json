  "devDependencies": {
    "@endo/bundle-source": "^2.4.2",
    "@endo/init": "^0.5.52",
    "@endo/marshal": "^0.8.1",
    "ava": "^5.1.0",
    "c8": "^7.12.0"
  },
  "files": [
    "LICENSE*",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
