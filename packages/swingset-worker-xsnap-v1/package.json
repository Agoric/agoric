{
  "name": "@agoric/swingset-worker-xsnap-v1",
  "version": "0.9.0",
  "description": "make swingset xsnap-v1 workers",
  "author": "Agoric",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./src/index.js",
  "scripts": {
    "build": "exit 0",
    "clean": "exit 0",
    "lint": "run-s --continue-on-error lint:*",
