    "lint:types": "tsc -p jsconfig.json",
    "lint-fix": "eslint --fix 'src/**/*.js' 'test/**/*.js'",
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0"
  },
  "dependencies": {
    "@agoric/assert": "^0.5.1",
    "@agoric/swingset-xsnap-supervisor": "^0.9.0",
