{
    "name": "fast-usdc",
    "private": true,
    "version": "0.1.0",
    "description": "Create an Agoric Javascript smart contract application",
    "type": "module",
    "files": [
        "contract"
    ],
    "scripts": {
        "build": "exit 0",
        "test": "ava",
        "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
        "test:xs": "exit 0",
        "lint-fix": "yarn lint:eslint --fix",
        "lint": "run-s --continue-on-error lint:*",
        "lint:types": "tsc",
        "lint:eslint": "eslint ."
    },
    "devDependencies": {
        "ava": "^5.3.0",
        "c8": "^9.1.0",
        "ts-blank-space": "^0.4.1"
    },
    "dependencies": {
        "@agoric/ertp": "^0.16.2",
        "@agoric/internal": "^0.3.2",
        "@agoric/network": "^0.1.0",
        "@agoric/orchestration": "^0.1.0",
        "@agoric/vats": "^0.15.1",
        "@agoric/vow": "^0.1.0",
        "@agoric/zoe": "^0.26.2",
        "@agoric/zone": "^0.2.2",
        "@endo/base64": "^1.0.7",
        "@endo/errors": "^1.2.5",
        "@endo/far": "^1.1.5",
        "@endo/marshal": "^1.5.3",
        "@endo/nat": "^5.0.10",
        "@endo/patterns": "^1.4.3",
        "@noble/hashes": "^1.5.0",
        "agoric": "^0.21.1"
    },
    "ava": {
        "extensions": {
            "js": true,
            "ts": "module"
        },
        "files": [
            "test/**/*.test.*"
        ],
        "nodeArguments": [
            "--import=ts-blank-space/register",
            "--no-warnings"
        ],
        "require": [
            "@endo/init/debug.js"
        ],
        "timeout": "20m"
    }
}
