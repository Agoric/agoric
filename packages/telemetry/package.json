{
  "name": "@agoric/telemetry",
  "version": "0.1.1",
  "description": "Agoric's telemetry implementation",
  "type": "module",
  "repository": "https://github.com/Agoric/agoric-sdk",
  "main": "./src/index.js",
  "scripts": {
    "build": "exit 0",
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "run-s --continue-on-error lint:*",
    "lint:types": "tsc -p jsconfig.json",
    "lint:eslint": "eslint '**/*.js'"
  },
  "bin": {
    "frcat": "./src/frcat-entrypoint.js"
  },
  "keywords": [],
  "author": "Agoric",
  "license": "Apache-2.0",
  "dependencies": {
    "@endo/init": "^0.5.36",
    "@opentelemetry/exporter-prometheus": "^0.16.0",
    "@opentelemetry/metrics": "^0.16.0",
    "bufferfromfile": "agoric-labs/BufferFromFile#Agoric-built"
  },
  "devDependencies": {
    "@endo/lockdown": "^0.1.8",
    "@endo/ses-ava": "^0.2.20",
    "ava": "^3.12.1",
    "c8": "^7.7.2",
    "tmp": "^0.2.1"
  },
  "eslintConfig": {
    "extends": [
      "@agoric"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "timeout": "20m"
  }
}
